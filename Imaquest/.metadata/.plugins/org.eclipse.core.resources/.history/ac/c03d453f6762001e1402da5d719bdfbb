import java.util.Random;

public class Gtya {
    public static String Gacha(int randomNumber) {
        if (randomNumber >= 0 && randomNumber <= 82) {
            return "★";
        } else if (randomNumber >= 83 && randomNumber <= 96) {
            return "★★";
        } else {
            return "★★★";
        }
    }

   
}
